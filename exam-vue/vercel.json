{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build:prod",
        "outputDirectory": "dist"
      }
    },
    {
      "src": "api/proxy.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/proxy.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*).js",
      "dest": "/$1.js"
    },
    {
      "src": "/(.*).css",
      "dest": "/$1.css"
    },
    {
      "src": "/(.*).png",
      "dest": "/$1.png"
    },
    {
      "src": "/(.*).jpg",
      "dest": "/$1.jpg"
    },
    {
      "src": "/(.*).svg",
      "dest": "/$1.svg"
    },
    {
      "src": "/(.*).ico",
      "dest": "/$1.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 